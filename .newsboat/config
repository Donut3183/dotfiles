highlight feedlist "---.*---" cyan default bold
highlight feedlist ".*0/0.." default default invis

refresh-on-startup yes

include /usr/share/doc/newsboat/contrib/colorschemes/inkpot

macro o set browser "/home/pontus/scripts/tag.sh %u"; open-in-browser; set browser "brave --new-window %u"

browser "brave --new-window %u"

unbind-key h
unbind-key j
unbind-key k
unbind-key l

bind-key h quit
bind-key j down
bind-key k up
bind-key l open
