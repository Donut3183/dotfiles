{
  "nasa": {
    "prefix": "nasa",
    "body": [
      "/*",
      " ********************************************************************************",
      " * Function:",
      " *   ${1:FunctionName}",
      " *",
      " * Purpose :",
      " *   ${2:Describe the purpose of the function.}",
      " *",
      " * Assumptions and Notes:",
      " *   ${3:List any assumptions, constraints, or important notes.}",
      " *",
      " * Inputs:",
      " *   ${4:List input parameters and their descriptions.}",
      " *",
      " * Outputs:",
      " *   ${5:List output parameters and what they represent.}",
      " *",
      " * Return:",
      " *   ${6:Description of the return value.}",
      " *",
      " ********************************************************************************",
      " */"
    ],
    "description": "NASA-style C function comment block with purpose, assumptions, inputs, outputs, and return value."
  },
  "C File Header Box": {
    "prefix": "header",
    "body": [
      "/*",
      " ********************************************************************************",
      " * File:        ${1:filename}",
      " *",
      " * Description: ${2:Short description}",
      " *",
      " * Author:      Pontus Svensson",
      " * Date:        ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      " * License:     MIT License",
      " ********************************************************************************",
      " */",
      "",
      "$0"
    ],
    "description": "Boxed file header for C source/header files"
  }
}
